[ENABLE]
aobscanmodule(aobMachinaDispatchingTime,Toukiden2_EN.exe,89 8E B0 BF 02 00 33 DB 48 8D BE EC BE 02 00 0F 1F 00)
registersymboL(aobMachinaDispatchingTime)
alloc(newmem,512,"Toukiden2_EN.exe"+12829E)

label(loop_set)
label(mission_time_code)
label(original_code)
label(return)


newmem:
mov [machina],rsi
xor ebx,ebx
cmp byte ptr [machina_flags+01],01 // set all materials to 9
jne mission_time_code
loop_set:
cmp word ptr [rsi+rbx*4+2BFB8],0000
js mission_time_code
mov byte ptr [rsi+rbx*4+2BFBA],09
inc bl
cmp bl,19
jnl mission_time_code
jmp loop_set
mission_time_code:
cmp byte ptr [machina_flags],01 // instant mission complete
jne original_code
xor rcx,rcx
original_code:
mov [rsi+0002BFB0],ecx
jmp return

machina:
db 00 00 00 00 00 00 00 00
registersymbol(machina)

machina_flags:
db 01 01
registersymbol(machina_flags)

aobMachinaDispatchingTime:
jmp newmem
nop
return:


[DISABLE]
aobMachinaDispatchingTime:
db 89 8E B0 BF 02 00

unregistersymbol(machina_flags)
unregistersymbol(machina)
unregistersymbol(aobMachinaDispatchingTime)
dealloc(newmem)
</AssemblerScript>
